{
  "include": [
    "./**/*",
    "../common.ts"
, "../../webpack/webpack.config.js"  ],
  "exclude": [
    "../node_modules/*"
  ],
  "compilerOptions": {
    "checkJs": false,
    "target": "es2015",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "sourceMap": true,
  }
}